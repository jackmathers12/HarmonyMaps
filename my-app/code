<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBt1UEJO-wc-C3cbklKE8DJF6xMlaYBj4E&callback=initMap"
    async defer></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBt1UEJO-wc-C3cbklKE8DJF6xMlaYBj4E&libraries=geometry">
</script>
    <div id="map"></div>
    <script>
      function initMap() {
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 18,
          center: new google.maps.LatLng(-39.08936,174.06052),
          disableDefaultUI: true,
          mapTypeId: 'satellite',
        });
        map.data.loadGeoJson("https://raw.githubusercontent.com/jackmathers12/HarmonyMaps/main/my-app/mapLayers.json");
        map.data.setStyle({
          fillColor: 'black',
          strokeWeight: 2
        });
        map.data.addListener('mouseover', function(event) {
          map.data.overrideStyle(event.feature, {fillColor: 'blue'});
          var infowindow = new google.maps.InfoWindow({
            content: event.feature.getProperty('')
          });
          infowindow.setPosition(event.latLng)
          infowindow.open(map);
          map.data.addListener('mouseout', function(event) {
            map.data.revertStyle();
            infowindow.close();
          });
        });
        map.data.addListener('click', function(event) {
          var url = 'https://www.google.com/search?q='+event.feature.getProperty('name');
          window.open(url, '_blank')
        });
      }
      google.maps.event.addDomListener(window, 'load', initialize);
    </script>
  </body>
</html>